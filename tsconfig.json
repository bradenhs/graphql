{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": "./",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "module": "commonjs",
    "jsx": "react",
    "sourceMap": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "plugins": [{ "name": "tslint-language-service" }]
  }
}
